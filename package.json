{
  "name": "comunitree-backend",
  "version": "1.0.0",
  "description": "Comunitree - JavaScript backend with Snowflake",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "start": "node server/index.js",
    "dev": "node --watch server/index.js",
    "dev:client": "npm --prefix client run dev",
    "build:client": "npm --prefix client run build",
    "init-db": "node server/db/init.js",
    "migrate-private-friends": "node server/db/migratePrivateAndFriends.js",
    "migrate-profile-config": "node server/db/migrateProfileAndConfig.js",
    "seed-locations": "node server/db/seedLocations.js"
  },
  "keywords": [
    "comunitree",
    "snowflake",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "snowflake-sdk": "^1.15.0",
    "socket.io": "^4.8.3"
  }
}
